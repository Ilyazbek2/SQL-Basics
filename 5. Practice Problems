SELECT * FROM students
WHERE city IN ('New York', 'Buffalo', 'Rochester');
âœ” Method 2: Using OR
sql
ÐšÐ¾Ð¿Ð¸Ñ€Ð¾Ð²Ð°Ñ‚ÑŒ
Ð ÐµÐ´Ð°ÐºÑ‚Ð¸Ñ€Ð¾Ð²Ð°Ñ‚ÑŒ
SELECT * FROM students
WHERE city = 'New York' OR city = 'Buffalo' OR city = 'Rochester';
âœ” Method 3: Using CASE
sql
ÐšÐ¾Ð¿Ð¸Ñ€Ð¾Ð²Ð°Ñ‚ÑŒ
Ð ÐµÐ´Ð°ÐºÑ‚Ð¸Ñ€Ð¾Ð²Ð°Ñ‚ÑŒ
SELECT *
FROM students
WHERE 
  CASE 
    WHEN city = 'New York' THEN 1
    WHEN city = 'Buffalo' THEN 1
    WHEN city = 'Rochester' THEN 1
    ELSE 0
  END = 1;
